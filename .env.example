# Copy this file to a new file called ".env" and fill in the values below.
# NEVER share your .env file or commit it to GitHub — it contains your secret API key.

# Your Anthropic API key — get this from https://console.anthropic.com
ANTHROPIC_API_KEY=sk-ant-your-key-here

# The URL of your Vercel frontend (e.g. https://predict-perfect.vercel.app)
# This controls which websites are allowed to call this backend.
ALLOWED_ORIGIN=https://your-vercel-app.vercel.app

# Port — leave this as 3001 for local development. Railway sets it automatically in production.
PORT=3001
